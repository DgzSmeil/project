<?xml version="1.0"?>    
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">       
<hibernate-mapping>      
    <class name="com.gs.pojo.Role_permission" table="t_role_permission">    
        <id name="id">     
            <generator class="uuid"></generator>    
        </id>    
        <property name="role_id" type="string" length="500"></property>  
        <property name="permission_id" type="string" length="50" ></property>
        <property name="created_time" type="date"></property>
    </class>    
        
</hibernate-mapping>