<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="likePackage" namespace="/like" extends="basePackage">
    	<action name="*" class="likeAction" method="{1}">
    		<result name="login">/WEB-INF/views/user/login.jsp</result>
    		<result name="addLike" type="json"></result>
    		<result name="findlike" type="json"></result>
    		<result name="likelist" type="json"></result>
    		<result name="del" type="json"></result>
    		<result name="all">/WEB-INF/views/like/like_list.jsp</result>
    		<result name="all2" type="json">/WEB-INF/views/like/like_list.jsp</result>
    	</action>
    </package>

</struts>