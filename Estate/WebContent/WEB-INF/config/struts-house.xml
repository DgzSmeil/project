<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="housePackage" namespace="/house" extends="json-default">
    	<action name="*" class="houseAction" method="{1}">
    		<result name="list" type="json"></result>
    		<result name="strap" >/WEB-INF/views/house/house_home.jsp</result>
    		<result name="add" >/WEB-INF/views/house/house_add.jsp</result>
    		<result name="all" >/WEB-INF/views/house/house_all.jsp</result>
    		<result name="look" >/WEB-INF/views/house/house_look.jsp</result>
    		<result name="addsave" >/WEB-INF/views/house/house_home.jsp</result>
    		<result name="update" >/WEB-INF/views/house/house_update.jsp</result>
    		<result name="updatesave" >/WEB-INF/views/house/house_home.jsp</result>
    		<result name="status" >/WEB-INF/views/house/house_home.jsp</result>
    		<result name="find" >/WEB-INF/views/house/house_home.jsp</result>
			<result name="lookHouse" >/WEB-INF/views/house/HouseLook.jsp</result>
    		<!-- managaer start -->
    		<result name="mgnhousepage">/WEB-INF/views/manager/manager_house.jsp</result>
    		<result name="returnjson" type="json"></result>
    		<!-- manager end -->
    	</action>
    </package>

</struts>